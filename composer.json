{    "name": "rafasales/buscador-cursos-alura",    "description": "Projeto que busca cursos no site da alura",    "type": "library",    "authors": [        {            "name": "salesrafa",            "email": "sales.rafa@gmail.com"        }    ],    "require": {        "symfony/dom-crawler": "^4.3",        "guzzlehttp/guzzle": "^6.3",        "symfony/css-selector": "^4.3"    },    "autoload": {        "psr-4": {            "Alura\\BuscadorDeCursos\\": "src/"        }    },    "require-dev": {        "phpunit/phpunit": "^7.5",        "squizlabs/php_codesniffer": "^3.4",        "phan/phan": "^2.2"    },    "scripts": {        "test": "phpunit tests/TestBuscadorDeCursos.php",        "cs": "phpcs --standard=PSR12 src/",        "phan": "phan --allow-polyfill-parser",        "check": [            "@phan",            "@cs",            "@test"        ],        "post-update-cmd": [            "@test"        ]    },    "scripts-descriptions": {        "check": "Roda as verificações do código. PHAN, PHPCS e PHPUNIT"    }}